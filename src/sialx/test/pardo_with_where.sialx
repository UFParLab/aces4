sial pardo_with_where

	special enable_all_rank_print
	predefined int norb
	aoindex mu     = 1: norb
	aoindex nu     = 1: norb
	aoindex lambda = 1: norb
	aoindex sigma  = 1: norb
	
	sip_barrier
	
	execute enable_all_rank_print
	
	pardo mu, nu, lambda, sigma
		WHERE mu      < nu
		WHERE lambda  < sigma
		WHERE mu     < lambda
		WHERE nu     != sigma
		WHERE nu     != lambda
		WHERE mu     != sigma

		print mu
		print nu
		print lambda
		print sigma
		println "------"

	endpardo mu, nu, lambda, sigma

	sip_barrier

endsial pardo_with_where
 sial gpu_contraction_small
	special print_block r
	special fill_block_cyclic wr

	aoindex i = 1,1
	aoindex j = 1,1
	aoindex k = 1,1
	aoindex l = 1,1
	aoindex m = 1,1
	
	temp a(i, j, k, l)
	temp b(j, k)
	local c(i, l)

	
	do k
		do j
			execute fill_block_cyclic b (j, k) 1.0
			#execute print_block b (j, k)
			do i
				do l
					allocate c(i, l)
					execute fill_block_cyclic a(i, j, k, l) 1.0			
					#execute print_block  a(i, j, k, l)
					
					gpu_on
					gpu_put a(i, j, k, l)
					gpu_put b(j, k)
					gpu_allocate c(i, l)
					
					c(i, l) = a(i, j, k, l) * b(j, k)

					gpu_get c(i, l)

					gpu_free a(i, j, k, l)
					gpu_free b(j, k)

					gpu_off
					
					execute print_block c(i,l)
					#execute print_block b(j, k)
					
				enddo l
			enddo i
		enddo j
	enddo k 


endsial gpu_contraction_small

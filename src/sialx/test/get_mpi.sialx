sial get_mpi
	special list_block_map
	special print_block r
	special get_my_rank w
	special enable_all_rank_print
	predefined int norb
	aoindex i = 1:norb
	aoindex j = 1:norb
	local a[i,j]
	local aa[i,j]
	distributed b[i,j]
	distributed c[i,j]
	scalar rank
	
	create b
	create c
	allocate a[*,*]
	
	println "Sending blocks"	
	pardo i, j
	print i
	print j
		a[i,j] = 42.0
		print a[i,j]
		put b[i,j] = a[i,j]
		put c[i,j] += a[i,j]
		put c[i,j] += a[i,j]
	endpardo i, j
	sip_barrier
	
	
	
	execute enable_all_rank_print
	#do i
	pardo i
	   do j
	      get b[i,j]
	      get c[i,j]
	      a[i,j] = b[i,j] + c[i,j]
	      print a[i,j]
	   enddo j
	#enddo i
	endpardo i

	sip_barrier
	execute get_my_rank rank
	print rank
	execute list_block_map
	delete b
	delete c
	
endsial get_mpi
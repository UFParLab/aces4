            endif 
            endif 
         ENDDO b 

        #ENDIF # icount <= nfrags 

      ENDDO jfrag 

     #DO b 
     #   execute return_sval wVirtB_frag(b) m  
     #   print_scalar m 
     #ENDDO b  

     #DO j 
     #   execute return_sval wOccB_frag(j) m  
     #   print_scalar m 
     #ENDDO j  
#
      ENDPROC SET_FRAG_PARMS    

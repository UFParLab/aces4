	Tixbj[i,mu,b,j] = 0.0  
#
	do nu  
#
	T1ixbj[i,mu,b,j] = Lixxj[i,mu,nu,j]*ca[nu,b]
	Tixbj[i,mu,b,j] += T1ixbj[i,mu,b,j]  
#
	enddo nu  
#
	prepare Vixbj[i,mu,b,j] = Tixbj[i,mu,b,j]
#
    enddo b 

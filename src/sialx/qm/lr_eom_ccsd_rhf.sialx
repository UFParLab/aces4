    t1pp[a1,i2]         = St1a[a1,i2]
    T2aiai[a,i2,a1,i3]  = St1a[a,i2]^tpp[a1,i3]
    T3aiai[a,i2,a1,i3]  = St1a[a,i3]^t1pp[a1,i2]

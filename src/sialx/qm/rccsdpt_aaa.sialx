     #   ca(mu,p) = Dca(mu,p) 
     #ENDDO p 
     #ENDDO mu 

     #DO p 
     #DO p1 
     #   GET            Fta(p,p1) 
     #   fock_a(p,p1) = Fta(p,p1) 
     #ENDDO p1 
     #ENDDO p 
      
      server_barrier
#
      PARDO a, a1, i, i1 
            REQUEST T2aiai[a,i,a1,i1]  

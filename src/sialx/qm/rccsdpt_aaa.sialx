      PROC COMP_E4T  
#     ------------- 
#
# ------------------------------------------------------------------------------ 
#
# Form Taaaiii and form energy contribution 
# ----------------------------------------- 
#
# Set the numerical factor 
# ------------------------ 
#
            pcount = 0
            fcount = 0
            if whichi < whichj 
               pcount = pcount + 1 
            endif 
            if whichj < kstart  
               pcount = pcount + 1 
            endif 
            if a < a1 
               fcount = fcount + 1 
            endif 
            if a1 < a2 
               fcount = fcount + 1 
            endif 
            if pcount == 0
               if fcount == 0
                  efact = 1.0
                  fact = 1.0
               endif
            endif

            if pcount == 0
               if fcount == 1
                  efact = 3.0
                  fact = 3.0
               endif
            endif

            if pcount == 1
               if fcount == 0
                  efact = 3.0
                  fact = 3.0
               endif
            endif

            if pcount == 1
               if fcount == 1
                  efact = 9.0
                  fact = 3.0
               endif
            endif

            if pcount == 2
               if fcount == 00
                  efact = 6.0
                  fact = 6.0
               endif
            endif

            if pcount == 0
               if fcount == 2
                  efact = 6.0
                  fact = 6.0
               endif
            endif

            if pcount == 2
               if fcount == 1
                  efact = 18.0
                  fact = 12.0
               endif
            endif

            if pcount == 1
               if fcount == 2
                  efact = 18.0
                  fact = 12.0
               endif
            endif
            if pcount == 2
               if fcount == 2
                  efact = 36.0
                  fact = 18.0
               endif
            endif 
#
#   Compute the energy now 
#   ---------------------- 
#
            kcount = 0 
            DO k1 
               kcount = kcount + 1 
               if kcount == kstart  
            jcount = 0 
            DO j1 
               jcount = jcount + 1 
               if jcount == whichj 
               icount = 0 
            DO i1 

               icount = icount + 1 
               if icount == whichi 

                  iicount = 0 
                  DO ii 
                     iicount = iicount + 1 
                     if iicount >= istart
                     if iicount <= iend
                        jjcount = 0 
                  DO jj 
                     jjcount = jjcount + 1 
                     if jjcount >= jstart
                     if jjcount <= jend
#
                        taiaiai[a,ii,a1,jj,a2,k1] = xaaaiii[a,a1,a2,k1,ii,jj] 

                        execute energy_denominator_rhf taiaiai[a,ii,a1,jj,a2,k1] fock_a  
#
                        taaaiii[a,a1,a2,k1,ii,jj] = taiaiai[a,ii,a1,jj,a2,k1]   

                        tpppp[a,a1,a2,k1]         = taaaiii[a,a1,a2,k1,ii,jj]
                        t1pppp[a,a1,a2,k1]        = xaaaiii[a,a1,a2,k1,ii,jj]
                        etemp                     = tpppp[a,a1,a2,k1]*t1pppp[a,a1,a2,k1] 
                        etemp                    *= efact 
                        esum                     += etemp 
#
                        xaaaiii[a,a1,a2,k1,ii,jj] = taaaiii[a,a1,a2,k1,ii,jj] 
#
                     endif 
                     endif 
                  ENDDO jj 
                     endif 
                     endif 
                  ENDDO ii 
#
               endif 

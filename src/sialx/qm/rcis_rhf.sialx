PROC normalize_RB1
#
server_barrier
do ksub
    normS = 0.0
    pardo a,i
        request RB1_a[ksub,a,i]
	Tkai[ksub,a,i] = RB1_a[ksub,a,i]
	normT  = Tkai[ksub,a,i]*RB1_a[ksub,a,i]
	normS += normT
    endpardo a,i
#
    server_barrier
    norm1 = 0.0
    norm2 = 1.0
    collective norm1 += normS
#    if norm1 > normcrit
	norm2 = (norm1)**(-0.5)
#    endif
#    print norm2
    Tk[ksub] = norm2
#
    server_barrier
    pardo a,i
	request RB1_a[ksub,a,i]
        Tai[a,i] = Tk[ksub]*RB1_a[ksub,a,i]
	put DRNai[a,i] = Tai[a,i]
    endpardo a,i
    server_barrier
#
    T1k[ksub] = 1.0
    pardo a,i
        get DRNai[a,i]
	Tkai[ksub,a,i] = T1k[ksub]*DRNai[a,i]
	prepare RB1_a[ksub,a,i] = Tkai[ksub,a,i]
    endpardo a,i
#
enddo ksub
server_barrier
delete DRNai
#
ENDPROC normalize_RB1

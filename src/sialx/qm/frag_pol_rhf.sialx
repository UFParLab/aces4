PROC HFIDENS
#
#     Procedure which computes the HF density without Davidson damping.
#     -----------------------------------------------------------------
#
restore_persistent DCa_g "ca_g"
sip_barrier
PARDO ifrag, mu, nu "Frag{i}{aa}{}"
#
#           alpha component
#           ---------------
#
    Tpq[mu,nu] = 0.0
#
    DO i
    where (int)SwMOA_frag[(index)i] == ifrag

#
       get DCa_g[nu,i]
       get DCa_g[mu,i]
       Txi[nu,i]   = DCa_g[nu,i]
       T1pq[mu,nu] = Txi[nu,i]*DCa_g[mu,i]
       Tpq[mu,nu] += T1pq[mu,nu]

#
    ENDDO i
#
    T1xx[mu,nu] = Tpq[mu,nu]
    PUT DHFa[mu,nu] = Tpq[mu,nu]
    PUT DHFb[mu,nu] = Tpq[mu,nu]
#
ENDPARDO ifrag, mu, nu
sip_barrier
pardo ifrag,mu,i "Frag{i}{ap}{}"
    get DCa_g[mu,i]
    put DCa[mu,i] = DCa_g[mu,i]
endpardo ifrag,mu,i
pardo ifrag,mu,a "Frag{i}{ap}{}"
    get DCa_g[mu,a]
    put DCa[mu,a] = DCa_g[mu,a]
endpardo ifrag,mu,a
sip_barrier
#
ENDPROC HFIDENS

PROC IGUESS
#
print "Forming Guess"
sip_barrier
pardo ifrag, mu,nu "Frag{i}{aa}{}"

	PUT Fpq_a[mu,nu] = 0.0
	PUT Fpq_b[mu,nu] = 0.0

endpardo ifrag, mu,nu
sip_barrier
pardo ifrag, mu,nu "Frag{i}{aa}{}"

	etemp = (scalar)ifrag
	execute return_h1_moi Txx[mu,nu] etemp
	if do_shielding >= 1
	    etemp = (scalar)ifrag
	    T2xx[mu,nu] = 0.0
	    execute return_h1frag T2xx[mu,nu] etemp
	    Txx[mu,nu] += T2xx[mu,nu]
	endif

	PUT Fpq_a[mu,nu] += Txx[mu,nu]
	PUT Fpq_b[mu,nu] += Txx[mu,nu]

endpardo ifrag, mu,nu
sip_barrier
#
ENDPROC IGUESS

#
      PROC DXY_CONT
#     -------------
#
# Contract with the second derivative integrals
# --------------------------------------
#

      flag = 2.0
      Do Ngrads
         DO Mgrads
            L_Hessian[Ngrads,Mgrads] = 0.0
         ENDO Mgrads
      ENDDO Ngrads

      PARDO mu, nu
#
            GET             Dhfa[mu,nu]
            GET             Dhfb[mu,nu]
            Txx[mu,nu]    = Dhfa[mu,nu]
            Txx[mu,nu]   += Dhfb[mu,nu]
            T1xx[mu,nu]   = 0.0

            execute  return_kin_ovl_hessintS t1xx[mu,nu] txx[mu,nu] L_Hessian flag
            DO Ngrads
               DO Mgrads
                  PUT Hessian[Mgrads,Ngrads] += L_Hessian[Mgrads,Ngrads]
               ENDDO Mgrads
            ENDDO Ngrads

            DO Ngrads
               DO Mgrads
                  L_Hessian[Ngrads,Mgrads] = 0.0
               ENDDO Mgrads
            ENDDO Ngrads

            execute return_nai_hessints txx[mu,nu] L_Hessian 

            Do Ngrads
               DO Mgrads
                  PUT Hessian[Ngrads,Mgrads] += L_Hessian[Mgrads,Ngrads]
               ENDO Mgrads
            ENDDO Ngrads
#
      ENDPARDO mu, nu
#
      ENDPROC DXY_CONT

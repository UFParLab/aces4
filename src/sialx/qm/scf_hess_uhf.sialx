#
# ---------------------------------------------------------------------------------
#
      PROC I_HF
#     --------
#
      create Ihf 
      sip_barrier 
#
      PARDO mu, nu, i
#
            Txi[nu,i] = 0.0
#
            DO i1
#
               T1xi[nu,i] = ca[nu,i1]*fock_a[i,i1]
               Txi[nu,i] -= T1xi[nu,i]
#
            ENDDO i1
#
            Txx[mu,nu]      = ca[mu,i]*Txi[nu,i]
            T1xx[mu,nu]     = Txx[mu,nu] 
            PUT Ihf[mu,nu] += Txx[mu,nu]
#
      ENDPARDO mu, nu, i
#
      PARDO mu, nu, j
#
            Txj[nu,j] = 0.0
#
            DO j1
#
               T1xj[nu,j] = cb[nu,j1]*fock_b[j,j1]
               Txj[nu,j] -= T1xj[nu,j]
#
            ENDDO j1
#
            Txx[mu,nu]      = cb[mu,j]*Txj[nu,j]
            T1xx[mu,nu]     = Txx[mu,nu]
            PUT Ihf[mu,nu] += Txx[mu,nu]
#
      ENDPARDO mu, nu, j
#
      sip_barrier 
#
      ENDPROC I_HF

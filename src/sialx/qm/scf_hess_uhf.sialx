#
      PROC HFDENS
#     -----------
#
      create Dhfa 
      create Dhfb 
      sip_barrier 
#
      PARDO mu, nu, i
#
            Txi[nu,i]        = ca[nu,i]
            Txx[mu,nu]       = ca[mu,i]*Txi[nu,i]
            PUT Dhfa[mu,nu] += Txx[mu,nu]
#
      ENDPARDO mu, nu, i
#
      PARDO mu, nu, j
#
            Txj[nu,j]        = cb[nu,j]
            Txx[mu,nu]       = cb[mu,j]*Txj[nu,j]
            PUT Dhfb[mu,nu] += Txx[mu,nu]
#
      ENDPARDO mu, nu, j
      allocate LDHFa[*,*] 
      allocate LDHFb[*,*] 
      sip_barrier 

      DO mu 
      DO nu 
         GET             Dhfa[mu,nu] 
         GET             Dhfb[mu,nu] 
         LDhfa[mu,nu] = Dhfa[mu,nu] 
         LDhfb[mu,nu] = Dhfb[mu,nu] 
      ENDDO nu 
      ENDDO mu 
#
      ENDPROC HFDENS

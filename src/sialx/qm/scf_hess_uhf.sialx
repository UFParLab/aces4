#
      PROC Qx_XX 
#     ---------- 
#
#     Form the virtual-occupied block of the Q array --> Qxai, Qxbj
#     -------------------------------------------------------------
#
#     Qxai first
#     ----------
#
      PARDO a, i
#
            tai[a,i] = 0.0 # Fxa[a,i]
#
            DO i1
#
               GET         Sxa[a,i1]
#
               DO i2
#
                  REQUEST     VSpipi[a,i,i2,i1] i 
                  GET         Sxa[i1,i2]
                  t1ai[a,i] = VSpipi[a,i,i2,i1]*Sxa[i1,i2]
                  tai[a,i] -= t1ai[a,i]
#
               ENDDO i2
#
               t1ai[a,i] = Sxa[a,i1]*Fock_a[i,i1]
               tai[a,i] -= t1ai[a,i]
            ENDDO i1
#
            DO j
            DO j1
#
               REQUEST     Vpiqj[a,i,j1,j] a 
               GET         Sxb[j,j1]
               t1ai[a,i] = Vpiqj[a,i,j1,j]*Sxb[j,j1]
               tai[a,i] -= t1ai[a,i]
#
            ENDDO j1
            ENDDO j
#
            PUT Qxai[a,i] += tai[a,i]
#
      ENDPARDO a, i
#
#     Qxbj next
#     ---------
#
      PARDO b, j
#
            tbj[b,j] = 0.0 #Fxb[b,j]
#
            DO j1
#
               GET         Sxb[b,j1]
#
               DO j2
#
                  REQUEST     VSqjqj[b,j,j2,j1] b 
                  GET         Sxb[j1,j2]
                  t1bj[b,j] = VSqjqj[b,j,j2,j1]*Sxb[j1,j2]
                  tbj[b,j] -= t1bj[b,j]
#
               ENDDO j2
#
               t1bj[b,j] = Sxb[b,j1]*Fock_b[j,j1]
               tbj[b,j] -= t1bj[b,j]
            ENDDO j1
#
            DO i
            DO i1
#
               REQUEST     Vpiqj[i1,i,b,j] b 
               GET         Sxa[i,i1]
               t1bj[b,j] = Vpiqj[i1,i,b,j]*Sxa[i,i1]
               tbj[b,j] -= t1bj[b,j]
#
            ENDDO i1
            ENDDO i
#
            PUT Qxbj[b,j] += tbj[b,j]
#
      ENDPARDO b, j
#
      ENDPROC Qx_XX  

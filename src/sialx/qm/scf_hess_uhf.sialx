      PROC SXY_CONT
#     -------------
#
# Contract with the second derivative integrals
# --------------------------------------
#
      flag = 3.0

      Do Ngrads
         Do Mgrads
            PUT Hessian[Mgrads,Ngrads] = 0.0
         Do Mgrads
      Enddo Ngrads
      sip_barrier 

      PARDO mu, nu
#
            GET             Ihf[mu,nu] 
            txx[mu,nu]    = Ihf[mu,nu] 
            t1xx[mu,nu]   = 0.0

            execute  return_kin_ovl_hessintS t1xx[mu,nu] txx[mu,nu] L_Hessian flag
            Do Ngrads
               Do Mgrads
                  PUT Hessian[Mgrads,Ngrads] += L_Hessian[Mgrads,Ngrads]
               Enddo Mgrads
            Enddo Ngrads

      ENDPARDO mu, nu
      sip_barrier 
#
      ENDPROC SXY_CONT

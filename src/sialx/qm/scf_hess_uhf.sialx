      PROC Uy_XX  
#     ----------- 
#
      create Uyaa 
      create Uybb 
      sip_barrier 
#
      PARDO mu, nu 
#
            execute overlap_der txx[mu,nu] 
#
            DO i 
#
               txp[mu,i] = txx[mu,nu]*ca[nu,i]  
#
               DO i1 
#
                  tpp[i1,i]       = txp[mu,i]*ca[mu,i1] 
                  tpp[i1,i]      *= -0.5  
                  PUT Uyaa[i1,i] += tpp[i1,i] 
#
               ENDDO i1  
#
            ENDDO i 
#
            DO j 
#
               txq[mu,j] = txx[mu,nu]*cb[nu,j]  
#
               DO j1 
#
                  tqq[j1,j]       = txq[mu,j]*cb[mu,j1] 
                  tqq[j1,j]      *= -0.5  
                  PUT Uybb[j1,j] += tqq[j1,j] 
#
               ENDDO j1  
#
            ENDDO j 
#
      ENDPARDO mu, nu 
#
      ENDPROC Uy_XX  

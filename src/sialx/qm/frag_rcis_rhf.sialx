PROC form_HB
print "Forming H*B"
do ksub
pardo ifrag, a,i "Frag{i}{pp}{}"
    put HB1_a[ksub,a,i] = 0.0
endpardo ifrag, a,i
enddo ksub
server_barrier

do ksub

    pardo fa,fi, fa1,fi1

        get HBAR_AJIB_aaaa[fi1,fa1,fa,fi]
	get RB1_a[ksub,fa,fi]

	Tkai[ksub,fa1,fi1] = HBAR_AJIB_aaaa[fi1,fa1,fa,fi]*RB1_a[ksub,fa,fi]

	put HB1_a[ksub,fa1,fi1] += Tkai[ksub,fa1,fi1]

    endpardo fa,fi, fa1,fi1

    pardo fa,fi, fb,fj 

	get HBAR_AJIB_aabb[fi,fa,fb,fj]
        get RB1_a[ksub,fb,fj]

        Tkai[ksub,fa,fi] = HBAR_AJIB_aabb[fi,fa,fb,fj]*RB1_a[ksub,fb,fj]
        put HB1_a[ksub,fa,fi] += Tkai[ksub,fa,fi] 

    endpardo fa,fi, fb,fj 

# add in cross terms
    pardo ifrag,jfrag, fa,fi, b,j "Frag{Nij}{pp}{pp}"

	get HBAR_AJIB_aabb[fi,fa,b,j]
        get RB1_a[ksub,b,j]

        Tkai[ksub,fa,fi]  = HBAR_AJIB_aabb[fi,fa,b,j]*RB1_a[ksub,b,j]
# forcing upper triangle.
	Tkai[ksub,fa,fi] *= 2.0
        put HB1_a[ksub,fa,fi] += Tkai[ksub,fa,fi] 

    endpardo ifrag,jfrag, fa,fi, b,j 

    pardo fa,fi 

        Tkai[ksub,fa,fi] = 0.0

	do fi1

	    get RB1_a[ksub,fa,fi1]
	    get HBAR_ii[fi,fi1]

	    T1kai[ksub,fa,fi]  = RB1_a[ksub,fa,fi1]*HBAR_ii[fi,fi1]
	    T1kai[ksub,fa,fi] *= -1.0
	    Tkai[ksub,fa,fi]  += T1kai[ksub,fa,fi]
	enddo fi1

        do fa1

	    get RB1_a[ksub,fa1,fi]
	    get HBAR_aa[fa,fa1]

	    T2kai[ksub,fa,fi] = RB1_a[ksub,fa1,fi]*HBAR_aa[fa,fa1]
	    Tkai[ksub,fa,fi] += T2kai[ksub,fa,fi]
        enddo fa1

	put HB1_a[ksub,fa,fi] += Tkai[ksub,fa,fi]

    endpardo fa,fi
enddo ksub
server_barrier
ENDPROC form_HB

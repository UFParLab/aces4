#
PROC form_BHB
print "Forming Final B*HB"
server_barrier

pardo ksub,ksub1
    put Gmat[ksub,ksub1] = 0.0
endpardo ksub,ksub1
server_barrier


pardo fa,fi 

    do ksub
    do ksub1
	get RB1_a[ksub,fa,fi]
	get HB1_a[ksub1,fa,fi]

	Tkk[ksub,ksub1] = RB1_a[ksub,fa,fi]*HB1_a[ksub1,fa,fi]

	put Gmat[ksub,ksub1] += Tkk[ksub,ksub1]
    enddo ksub1
    enddo ksub

endpardo fa,fi

server_barrier
ENDPROC form_BHB
